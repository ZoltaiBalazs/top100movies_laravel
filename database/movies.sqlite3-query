drop table if exists movies;
create table movies (
    Id integer primary key,
    Title text,
    Image text,
    Runtime text,
    Released text,
    Rated text,
    Imdb_Id text unique,
    Plot text,
    Director text,
    Actors text,
    Imdb_rating numeric,
    Writer text
);

